<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="FindReplacePanel">
		<object class="wxBoxSizer" variable="BoxSizer1" member="no">
			<object class="sizeritem">
				<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
					<label>Find - F3</label>
					<object class="sizeritem">
						<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="m_findTextCtrl" member="yes" />
						<flag>wxRIGHT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>2</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON1" variable="FindRevButton" member="yes">
							<label>&lt;</label>
							<size>40,-1</size>
							<tooltip>Find Previous</tooltip>
							<help>Find Previous</help>
							<handler function="OnFindRevButtonClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxRIGHT|wxALIGN_TOP</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_FIND_FWD_BUTTON" variable="FindFwdButton" member="yes">
							<label>&gt;</label>
							<size>40,-1</size>
							<tooltip>Find Next (F3)</tooltip>
							<help>Find Next (F3)</help>
							<handler function="OnFindFwdButtonClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALIGN_TOP</flag>
						<border>5</border>
					</object>
				</object>
				<flag>wxLEFT|wxRIGHT|wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxStaticBoxSizer" variable="StaticBoxSizer2" member="no">
					<label>Replace with</label>
					<object class="sizeritem">
						<object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="m_replaceTxtCtrl" member="yes" />
						<flag>wxRIGHT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>2</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON3" variable="ReplaceButton" member="yes">
							<label>Replace</label>
							<tooltip>Replace one</tooltip>
							<help>Replace one</help>
							<handler function="OnReplaceButtonClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxRIGHT|wxALIGN_TOP</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BUTTON5" variable="ReplaceAllButton" member="yes">
							<label>All</label>
							<tooltip>Replace All</tooltip>
							<help>Replace All</help>
							<handler function="OnReplaceAllButtonClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxALIGN_TOP</flag>
						<border>5</border>
					</object>
				</object>
				<flag>wxEXPAND</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
